{
  "name": "nucssa-theme",
  "version": "1.0.0",
  "description": "Theme for NUCSSA WordPress Portal",
  "author": "NUCSSA IT",
  "repository": "https://github.com/NUCSSA/nucssa-portal-theme",
  "license": "UNLICENSED",
  "scripts": {
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
