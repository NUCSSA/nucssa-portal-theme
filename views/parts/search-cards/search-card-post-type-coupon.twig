<article class="post-type-{{ post.post_type }} {{ post.class }} wp-block-nucssa-coupon {{ post.meta('style') }}">
  <div class="coupon-preview">

    {% set style = post.meta('style') ?? 'is-style-style-1-pattern-1' %}

    {% if 'is-style-style-1' in style %}
      {% include "parts/coupon-style-1.twig" %}
    {% else %}
      {% include "parts/coupon-style-2.twig" %}
    {% endif %}

  </div>

</article>